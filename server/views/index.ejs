<!DOCTYPE html>
<html lang="ru">
<head>
  <meta charset="UTF-8">
  <title>CHAT</title>

  <link rel="stylesheet" href="stylesheets/index.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.7.3/socket.io.js"></script>
  <script src="https://travistidwell.com/jsencrypt/bin/jsencrypt.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/crypto-js/3.1.9-1/crypto-js.js"></script>
  <script src="javascripts/FileSaver.min.js"></script>
</head>
<body>
<div class="header">
  <p id="header_label">CHAT</p>
</div>
<div class="signin">
  <div class="form_signin">
    <form class="form" method="get" action='chat'>
      <div class="header_signin">
        <p class="labels signin_label">ВХОД</p>
      </div>
      <div class="content_signin">
        <input id="sn_login"  class="signin_input login" name="login" placeholder="ЛОГИН">
        <input id="sn_password" type="password"  class="signin_input password" name="password" placeholder="ПАРОЛЬ">
        <!--<input id="sn_file"  class="file" type="file" name="file">-->
        <button type="button" class="button button_signin" name="button_signin">ВОЙТИ</button>
      </div>
    </form>
  </div>
  <div class="form_reg">
    <form class="form" method="get" >
      <div class="header_reg">
        <p class="labels reg_label">РЕГИСТРАЦИЯ</p>
      </div>
      <div class="content_reg">
        <input id="reg_login"  class="signin_input reg new_login" name="new_login" placeholder="ЛОГИН">
        <input id="reg_email" class="signin_input reg email" name="email" placeholder="ЭЛ.ПОЧТА">
        <input id="reg_password"  type="password"  class="signin_input reg password_reg" name="password_reg" placeholder="ПАРОЛЬ">
        <button type="button" class="button button_reg" name="login">ЗАРЕГИСТРИРОВАТЬСЯ</button>
      </div>
    </form>
  </div>
</div>
<script src="javascripts/index.js"></script>
</body>
</html>
